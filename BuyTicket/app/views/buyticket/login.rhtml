</head>
<body>

<div id="divcontainer">
<%= render :partial => '/includes/header' %>

<div id="content_part">
<div id="left_part">
 <script type="text/javascript">
 jQuery(function(){
 		jQuery('div#ifM_wrapper_1').fastFind(
 		{
 			url:	'../includes/',
 			params:	{ 'current':1,'previous':false,'apiKey':'8cfd86175' }
 		});
 		jQuery('.ifM_header a').click(function () {
 			var el = jQuery(this).parents('div.ifM_box').get(0);
 			jQuery(el).fastFindToggle(true);
 			return false;
 		});
 });
 </script>
 <div id="leftbox">
  <h1 class="aligntext">Browse Categories</h1>
  <div class="ifM_pager">
                            <a href="#" rel="Up" title="Scroll Up" style="display:none"><img src="/resources/images/arrow_up.gif" alt="Scroll Up"/></a>
                        </div>
                        <div class="ifM_content">
                            <a href="http://{'current':4,'previous':1,'apiKey':'bd51b0648d268122996b9e68cfd86175','client':'ActiveSpotLight'}" class="ifM_more">Browse by Movie</a>
                            <a href="http://{'current':2,'previous':1,'apiKey':'bd51b0648d268122996b9e68cfd86175','client':'ActiveSpotLight'}" class="ifM_more">Browse by Music</a>
                            <a href="http://{'current':3,'previous':1,'apiKey':'bd51b0648d268122996b9e68cfd86175','client':'ActiveSpotLight'}" class="ifM_more">Browse by Drama</a>
                            <a href="http://{'current':5,'previous':1,'apiKey':'bd51b0648d268122996b9e68cfd86175','client':'ActiveSpotLight'}" class="ifM_more">More Options..</a>
                        </div>
                        <div class="ifM_pager">
                            <a href="#" rel="Down" title="Scroll Down" style="display:none"><img src="/resources/images/arrow_dn.gif" alt="Scroll Up"/></a>
                        </div>
                        <div class="ifM_back">
                            <a href="http://{'previous':1}">Â« Back</a>
                        </div>
 </div>

 </div>
<div id="middle_part">
 <div id="openbox">
 	<h1 class="aligntext">Adminstrator Login</h1>
  
  	
  		<% if flash[:notice] -%>
		<div id="notice"><%= flash[:notice] %></div>
		<% end -%>
  	
  <table>
    <% form_tag do %>
     <tr>
      <td><label for="name">Administrator Name:</label></td>
      <td><%= text_field_tag :name, params[:name] %></td>
     </tr>
     <tr>
     <td><label for="password">Password:</label></td>
      <td><%= password_field_tag :password, params[:password] %></td>
     </tr>
     <tr>
      <td>&nbsp;</td>
      <td>
      <%= submit_tag "Login" %>
      </td>
     </tr>
      <% end %>  
	  
	  </table>
	  

</div>
</div>
<div id="right_part">
 <div id="rightbox">
   <h1 class="aligntext">Login / Register</h1>
    <table align="center" border="0" cellpadding="2" cellspacing="0">
     <tr>
      <td class="smalltxt">Candidate:</td>
      <td align="right">
      <%= link_to "Login", :action =>'login' %>&nbsp;|&nbsp;<%= link_to "Register", :action =>'register' %></td>
     </tr>
     <tr>
      <td class="smalltxt">Staff:</td>
      <td align="right">
      <%= link_to "Login", :action =>'login' %>&nbsp;|&nbsp;<%= link_to "Register", :action =>'register' %></td>
      </tr>
    </table>
   </div>

 <div id="rightbox">
