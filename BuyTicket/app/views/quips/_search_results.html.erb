<table>
  <tr>
  <% for column in Quip.content_columns %>
    <th><%= column.human_name %></th>
  <% end %>
  </tr>
<% for quip in @quips %>
  <tr>
  <% for column in Quip.content_columns %>
    <td><%=h quip.send(column.name) %></td>
  <% end %>
    <td><%= link_to 'Show', :action => 'show', :id => quip %></td>
    <td><%= link_to 'Edit', :action => 'edit', :id => quip %></td>
  </tr>
<% end %>
</table>
