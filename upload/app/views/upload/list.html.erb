 <% for list_item in @list_item %> 
<tr>
	<td><%= link_to list_item.block_name, :controller => 'content', :action => 'show', :id=>list_item.id %></td>
	<td><%= list_item.post_code %></td>
	<td><%= list_item.build_date %></td>
	<% category_name = h(list_item.category_name) %>
	
	<% @category_id = Upload.find_by_sql("select category_id from uploads where block_id=11")
		cat_id = @category_id.length
	%>
	<% for cat_ids in @category_id %>
	 <% asbes = h(cat_ids.category_id).to_i %>
	 <% if asbes=3 %>
	   <td><p>This is Asbestos docs</p></td>
	 <% else %>
	 	<td> Not upload</td>
	<% end %>
	   
	 <% if cat_ids.category_id==2 %>
	  <td> <p>This is FR docs</p>  </td>
	  <% else %>
	 	<td> Not upload</td>
	<% end %>  
	
	<% if cat_ids.category_id==1 %>
	  <td> <p>This is Manage docs</p>  </td>
	   <% else %>
	   <td> Not upload</td>
	   
	 <% end %>
		Cat_id:<%= h(cat_ids.category_id) %>|
	<% end %>
